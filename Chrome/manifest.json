{
  "manifest_version": 3,
  "name": "Discord Quest Hunter",
  "version": "1.0.0",
  "description": "",
  "permissions": [
    "tabs",
    "scripting",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "*://*.discord.com/*",
    "*://*.discordapp.com/*",
    "*://*.discord.gg/*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["main.js"],
      "matches": [ "*://*.discord.com/*", "*://*.discordapp.com/*", "*://*.discord.gg/*"]
    }
  ],
  "icons": { "48": "icon48.png" },
  "background": { "service_worker": "background.js" },
  "action": {
    "default_popup": "popup.html"
  }
}
